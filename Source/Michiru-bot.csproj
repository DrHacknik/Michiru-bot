<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <AssemblyName>Michiru-bot</AssemblyName>
    <RootNamespace>Michiru-bot</RootNamespace>
    <RuntimeModifier>linux-x64</RuntimeModifier>
    <Platforms>AnyCPU;x86</Platforms>
    <StartupObject>Michiru.Program</StartupObject>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|x64'">
    <AllowUnsafeBlocks>false</AllowUnsafeBlocks>
    <Optimize>false</Optimize>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Discord.Net" Version="3.5.0" />
    <PackageReference Include="Discord.Net.Commands" Version="3.5.0" />
    <PackageReference Include="Discord.Net.Core" Version="3.5.0" />
    <PackageReference Include="Discord.Net.Rest" Version="3.5.0" />
    <PackageReference Include="Discord.Net.Webhook" Version="3.5.0" />
    <PackageReference Include="Discord.Net.WebSocket" Version="3.5.0" />
    <PackageReference Include="Ionic.Zip" Version="1.9.1.8" />
  </ItemGroup>
</Project>
